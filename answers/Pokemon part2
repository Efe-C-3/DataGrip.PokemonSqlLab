SELECT name, primary_type FROM pokemons;
SELECT secondary_type FROM pokemons WHERE name = 'Rufflet';
SELECT name FROM pokemons where trainerID = 303 in trainers

"""What are the names of the pokemon that belong to the trainer
with trainerID 303?"""

SELECT COUNT(secondary_type) FROM pokemons JOIN types  ON  pokemons.id = (types.id = "Poison");
SELECT primary_type, COUNT(*) FROM pokemons GROUP BY primary_type;

